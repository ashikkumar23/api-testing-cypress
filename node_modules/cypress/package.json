{
  "name": "cypress",
  "version": "15.3.0",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "node dist/index.js --exec install",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";"
  },
  "dependencies": {
    "@cypress/request": "^3.0.9",
    "@cypress/xvfb": "^1.2.4",
