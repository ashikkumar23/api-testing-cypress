    "commander": "^6.2.1",
    "common-tags": "^1.8.0",
    "dayjs": "^1.10.4",
    "debug": "^4.3.4",
    "enquirer": "^2.3.6",
    "eventemitter2": "6.4.7",
    "execa": "4.1.0",
    "executable": "^4.1.1",
    "extract-zip": "2.0.1",
    "figures": "^3.2.0",
    "fs-extra": "^9.1.0",
    "hasha": "5.2.2",
    "is-installed-globally": "~0.4.0",
    "listr2": "^3.8.3",
    "lodash": "^4.17.21",
    "log-symbols": "^4.0.0",
